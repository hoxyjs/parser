{
  "name": "typescript-boilerplate",
  "version": "1.0.1",
  "main": "dist/bundle.js",
  "repository": "https://github.com/9oelM/typescript-boilerplate.git",
  "author": "Joel M <hj923@hotmail.com>",
  "scripts": {
    "start": "webpack && node dist/bundle.js",
    "watch": "./node_modules/.bin/npm-run-all -p webpack:watch forever",
    "test": "jest --passWithNoTests",
    "lint": "npm run tslint",
    "lint:config": "eslint --fix ./*.js",
    "prettier": "prettier --write 'src/**/*{.ts,.tsx}'",
    "tslint": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'",
    "webpack:watch": "webpack --watch",
    "forever": "forever --watch dist/bundle.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@types/dotenv": "^6.1.1",
    "@types/exceljs": "^0.5.3",
    "@types/jest": "^24.0.15",
    "@types/mongodb": "^3.1.31",
    "@types/mongoose": "^5.5.11",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "forever": "^1.0.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "exceljs": "^1.14.0",
    "mongoose": "^5.6.8"
  }
}
